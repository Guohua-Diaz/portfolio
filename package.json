{
  "name": "react-portfolio-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000",
    "build": "next build",
    "export": "next build && next export -o dist",
    "start": "next start",
    "serve": "npm run export && serve ./dist",
    "deploy": "npm run export && npx surge --domain `cat CNAME` dist"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "next": "12.1.6",
    "postcss": "^8.4.13",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-iframe": "^1.8.0",
    "react-player": "^2.10.1",
    "react-youtube": "^9.0.2",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "surge": "^0.23.1"
  }
}
